{
  "name": "string-interp",
  "version": "0.3.5",
  "description": "Shell-style string interpolation",
  "repository": "https://github.com/stanford-oval/string-interp",
  "author": "Giovanni Campagna <gcampagn@cs.stanford.edu>",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "coveralls": "^3.0.0",
    "deep-equal": "^2.0.1",
    "eslint": "^7.1.0",
    "nyc": "^15.0.0",
    "pegjs": "~0.10.0",
    "q": "^1.5.1",
    "seedrandom": "^3.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typedoc": "^0.20.7",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "lint": "eslint ./lib",
    "test": "nyc ts-node -P tsconfig.json ./test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "pegjs -o lib/grammar.js lib/grammar.pegjs && tsc --build tsconfig.json"
  },
  "dependencies": {
    "thingtalk-units": "^0.2.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "lib/grammar.js",
      "test/**",
      "test.js",
      "coverage/**"
    ]
  }
}
